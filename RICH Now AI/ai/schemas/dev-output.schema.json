{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "DevOutput",
  "type": "object",
  "required": [
    "files",
    "tests",
    "docs"
  ],
  "properties": {
    "files": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "tests": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "docs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "notes": {
      "type": "string"
    }
  }
}